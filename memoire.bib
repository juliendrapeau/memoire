  keywords = {Quantum information,Quantum mechanics},
  file = {/home/local/USHERBROOKE/draj1605/snap/zotero-snap/common/Zotero/storage/WR6XAPWX/McClean et al. - 2018 - Barren plateaus in quantum neural network training.pdf}
}

