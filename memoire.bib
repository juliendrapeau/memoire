@article{anschuetzBarrenPlateausQuantum2022a,
  title = {Beyond {{Barren Plateaus}}: {{Quantum Variational Algorithms Are Swamped With Traps}}},
  shorttitle = {Beyond {{Barren Plateaus}}},
  author = {Anschuetz, Eric R. and Kiani, Bobak T.},
  date = {2022-12-15},
